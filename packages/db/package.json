{
  "name": "@repo/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "db:up": "podman compose up -d",
    "db:generate": "bunx --bun prisma generate",
    "db:push": "dotenv -e .env.local -- bunx --bun prisma db push",
    "db:push:prod": "dotenv -e .env.production -- bunx --bun prisma db push",
    "db:studio": "dotenv -e .env.local -- bunx --bun prisma studio",
    "db:local": "dotenv -e .env.local -- bun",
    "db:production": "dotenv -e .env.production -- bun"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@prisma/extension-accelerate": "^3.0.0"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "dotenv-cli": "^11.0.0",
    "prisma": "^6.19.0",
    "tsup": "^8.5.1",
    "typescript": "^5.7.0"
  }
}