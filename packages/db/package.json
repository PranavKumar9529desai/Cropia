{
  "name": "@repo/db",
  "version": "0.0.0",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./index.ts": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "db:up": "podman compose up -d",
    "db:generate": "prisma generate",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev --skip-generate",
    "db:deploy": "dotenv -e .env.production -- prisma migrate deploy",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "db:studio": "dotenv -e .env.local -- prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "dotenv-cli": "^11.0.0",
    "prisma": "^5.0.0",
    "tsup": "^8.5.1"
  }
}